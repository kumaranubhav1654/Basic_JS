// Array Destucturing
//swap without using 3rd variable

let a = 5;
let b = 8;

[a, b] = [b, a];

console.log(`a is ${a} and b is ${b}`);

//Merge 2 arrays

const arr1 = [1, 5, 3, 2];
const arr2 = [10, 56, 33, 24];

//concat
const concatArr = arr1.concat(arr2);
console.log(concatArr);

//spread operator

const resultArr = [...arr1, ...arr2];
console.log(resultArr);

//sort array

resultArr.sort((a, b) => {
  return a - b;
});
